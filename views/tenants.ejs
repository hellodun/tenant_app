<body>
  <nav>
    <div>
      <a href="/">
        <h1>Tenants App</h1>
      </a>
    </div>
    <ul>
      <li><a href="/floorss">Floors</a></li>
      <li><a href="/tenantss">Tenants</a></li>
    </ul>
  </nav>

  <h3>Enter new tenant details</h3>
  <form>
    <label for="name">Enter name</label>
    <input type="text" name="name" id="name">
    <label for="email">Enter email</label>
    <input type="email" name="email" id="email">
    <label for="phone">Enter phone number</label>
    <input type="number" name="phone" id="phone">
    <label for="id">Enter National ID no./Passport no.</label>
    <input type="number" name="id" id="id">
    <label for="floor">Enter the floor living in</label>
    <input type="number" name="floor" id="floor">
    <label for="status">Select rent status</label>
    <select name="status" id="status">
      <option value="pain">Paid</option>
      <option value="Pending">Pending</option>
    </select>
    <input type="submit">
  </form>
</body>

</html>